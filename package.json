{
  "name": "seed4j",
  "version": "2.2.1-SNAPSHOT",
  "description": "seed4j",
  "homepage": "https://seed4j.com/",
  "bugs": "https://github.com/seed4j/seed4j/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/seed4j/seed4j.git"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/seed4j"
  },
  "license": "Apache-2.0",
  "type": "module",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:tikui": "tikui-core build",
    "build:tsc": "vue-tsc -p tsconfig.build.json --noEmit",
    "build:vite": "vite build --emptyOutDir",
    "dev": "npm-run-all --parallel dev:*",
    "dev:tikui": "tikui-core serve",
    "dev:vite": "vite",
    "e2e": "cypress open --config-file src/test/webapp/e2e/cypress-config.ts",
    "e2e:headless": "cypress run --headless --config-file src/test/webapp/e2e/cypress-config.ts",
    "format": "npm run prettier:format",
    "glyph:build": "rimraf .fontello-session && fontello-cli install --config src/main/glyph/config.json --font src/main/glyph/font --css src/main/glyph/css",
    "glyph:open": "rimraf .fontello-session && fontello-cli open --config src/main/glyph/config.json",
    "lint": "eslint . --fix --plugin eslint-plugin-pug src/main/style/**/*.pug && npm run lint:sass && npm run prettier:format",
    "lint:ci": "eslint . && npm run prettier:check",
    "lint:ci:sass": "stylelint '**/*.{css,scss}'",
    "lint:sass": "npm run lint:ci:sass -- --fix",
    "prepare": "husky",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier --write .",
    "preview": "vite preview",
    "start": "npm run dev",
    "test": "npm run test:watch",
    "test:component": "concurrently -k -s first -n dev,test \"npm run dev\" \"wait-on http://localhost:9000 && cypress open --e2e --config-file src/test/webapp/component/cypress-config.ts\"",
    "test:component:headless": "concurrently -k -s first -n dev,test \"npm run dev\" \"wait-on http://localhost:9000 && cypress run --headless --config-file src/test/webapp/component/cypress-config.ts\"",
    "test:coverage:check": "npm-run-all test:coverage:clean test:coverage:copy:* test:coverage:merge test:coverage:report",
    "test:coverage:clean": "rimraf target/.nyc_output",
    "test:coverage:copy:component": "cpy --flat target/frontend-coverage/components-tests/coverage-final.json target/.nyc_output/ --rename=component-{{basename}}",
    "test:coverage:copy:unit": "cpy --flat target/frontend-coverage/unit-tests/coverage-final.json target/.nyc_output/ --rename=unit-{{basename}}",
    "test:coverage:merge": "nyc merge target/.nyc_output target/.nyc_output/frontend-combined-coverage.json",
    "test:coverage:report": "nyc report",
    "test:unit:coverage": "vitest run --coverage",
    "test:watch": "vitest --",
    "watch:": "npm-run-all --parallel watch:*",
    "watch:test": "npm run test:watch",
    "watch:tsc": "npm run build:tsc -- --watch"
  },
  "dependencies": {
    "axios": "1.13.2",
    "mitt": "3.0.1",
    "piqure": "2.1.2",
    "placeholder-loading": "0.7.0",
    "vue": "3.5.26",
    "vue-router": "4.6.4"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.14.7",
    "@eslint/js": "9.39.2",
    "@prettier/plugin-pug": "3.4.2",
    "@prettier/plugin-xml": "3.4.2",
    "@tikui/core": "7.0.0",
    "@typescript-eslint/eslint-plugin": "8.52.0",
    "@typescript-eslint/parser": "8.52.0",
    "@vitejs/plugin-vue": "6.0.3",
    "@vitest/coverage-istanbul": "4.0.16",
    "@vue/test-utils": "2.4.6",
    "concurrently": "9.2.1",
    "cpy-cli": "6.0.0",
    "cypress": "15.8.2",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-cypress": "5.2.1",
    "eslint-plugin-pug": "1.2.5",
    "eslint-plugin-vue": "10.6.2",
    "fontello-cli": "0.6.2",
    "globals": "17.0.0",
    "husky": "9.1.7",
    "jsdom": "27.4.0",
    "lint-staged": "16.2.7",
    "npm-run-all2": "8.0.4",
    "postcss-scss": "4.0.9",
    "prettier": "3.7.4",
    "prettier-plugin-gherkin": "3.1.3",
    "prettier-plugin-java": "2.8.1",
    "prettier-plugin-organize-imports": "4.3.0",
    "prettier-plugin-packagejson": "2.5.20",
    "prettier-plugin-sh": "0.18.0",
    "rimraf": "6.1.2",
    "sass": "1.97.2",
    "stylelint": "16.26.1",
    "stylelint-config-concentric-order": "5.2.1",
    "stylelint-config-standard-scss": "16.0.0",
    "tikuidoc-tikui": "9.0.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.52.0",
    "vite": "7.3.1",
    "vite-plugin-istanbul": "7.2.1",
    "vitest": "4.0.16",
    "vitest-sonar-reporter": "3.0.0",
    "vue-tsc": "3.2.2",
    "wait-on": "9.0.3"
  },
  "engines": {
    "node": ">=20"
  }
}
